                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.CardView>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:clipToPadding="false"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_weight="1">

                    <android.support.v7.widget.CardView
                        android:id="@+id/diets"
                        android:layout_width="110dp"
                        android:layout_height="135dp"
                        android:layout_margin="10dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/customborder"
                        android:clickable="true"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardElevation="5dp">

                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/customborder"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="44dp"
                                android:layout_height="44dp"
                                android:background="@drawable/cerclepink"
                                android:padding="5dp"
                                android:src="@drawable/foood" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:text="Diets"
                                android:textStyle="bold" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_margin="5dp"
                                android:background="@color/lightgray" />

                            <TextView
                                style="@style/TextColor"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:padding="3dp"
                                android:text="ideal diets for mother" />

                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                        android:id="@+id/excercise"
                        android:layout_width="110dp"
                        android:layout_height="135dp"
                        android:layout_margin="10dp"
                        android:layout_weight="0.5"
                        android:clickable="true"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardElevation="5dp">

                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/customborder"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="44dp"
                                android:layout_height="44dp"
                                android:background="@drawable/cerclepink"
                                android:padding="5dp"
                                android:src="@drawable/exercise"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:text="Exercises"
                                android:textStyle="bold" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_margin="5dp"
                                android:background="@color/lightgray" />

                            <TextView
                                style="@style/TextColor"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:padding="0dp"
                                android:text="Ideal excercises for mother" />

                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.CardView>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:clipToPadding="false"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_weight="1">

                    <android.support.v7.widget.CardView
                        android:id="@+id/locatehospital"
                        android:layout_width="110dp"
                        android:layout_height="135dp"
                        android:layout_margin="10dp"
                        android:layout_weight="0.5"
                        android:clickable="true"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardElevation="5dp">

                        <android.support.v7.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/customborder"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="44dp"
                                android:layout_height="44dp"
                                android:background="@drawable/cerclepink"
                                android:padding="5dp"
                                android:src="@drawable/hospital" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:text="Locate Hospital"
                                android:textStyle="bold" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_margin="5dp"
                                android:background="@color/lightgray" />

                            <TextView
                                style="@style/TextColor"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:padding="3dp"
                                android:text="Nearby Hospitals" />

                        </android.support.v7.widget.LinearLayoutCompat>

                    </android.support.v7.widget.CardView>

